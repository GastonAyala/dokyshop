{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "DokyShop es un E-commerce especializado en la venta de accesorios, vestimenta y juguetes para mascotas. Nuestro equipo de desarrollo ha diseñado esta plataforma con tecnologías modernas para garantizar una experiencia fluida y segura.",
  "main": "./src/app.js",
  "scripts": {
    "start": "npm run env:prod && node ./src/bin/www",
    "db:reset:dev": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "db:reset:prod": "npm run env:prod && sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "env:prod": "cross-env NODE_ENV=production",
    "server-dev": "nodemon ./src/bin/www",
    "server-db-prod": "npm run env:prod && nodemon ./src/bin/www",
    "client-dev": "cd dashboard && npm run dev",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "test": "cross-env NODE_ENV=production node ./src/bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GastonAyala/dokyshop.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-partials": "^0.3.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "global": "^4.4.0",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.7",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.0",
    "passport": "^0.7.0",
    "passport-google-oauth": "^2.0.0",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "sequelize-paginate": "^1.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
